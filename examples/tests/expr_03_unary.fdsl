// ----------- Minimal header -----------
Server TestAPI
  host: "localhost"
  port: 8080
  cors: "http://localhost:3000"
end

RESTEndpoint Soil1
  url: "http://localhost:8080/api/soil/z1"
  verb: GET
end

RESTEndpoint Soil2
  url: "http://localhost:8080/api/soil/z2"
  verb: GET
end

Entity MoistureZone1
  source: Soil1
  attributes:
    - timestamp: datetime
    - moisture_percent: float
end

Entity MoistureZone2
  source: Soil2
  attributes:
    - timestamp: datetime
    - moisture_percent: float
end

// Entity ------------------------------------------------

Entity E3
  inputs:
    - z: MoistureZone1
  attributes:
    - u1 = - ( - z.moisture_percent)
    - u2 = not (z.moisture_percent < 30)
end