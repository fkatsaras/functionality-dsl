// Test: Source URL path param without definition
// Expected: FAIL - Path parameter in URL but not defined in parameters block
Server TestServer
  host: "localhost"
  port: 8080
end

Entity Data
  attributes:
    - id: string;
end

Endpoint<REST> GetItem
  path: "/api/items/{id}"
  method: GET
  parameters:
    path:
      - id: string
  response:
    type: object
    entity: Data
end

Source<REST> ItemAPI
  url: "http://api.example.com/items/{id}"
  method: GET
  response:
    type: object
    entity: Data
end
