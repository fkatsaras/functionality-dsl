// Test: Valid Source<REST> with parameters
// Expected: PASS
Server TestServer
  host: "localhost"
  port: 8080
end

Entity UserData
  attributes:
    - id: string;
    - name: string;
end

Endpoint<REST> GetUser
  path: "/api/users/{userId}"
  method: GET
  parameters:
    path:
      - userId: string
  response:
    type: object
    entity: UserData
end

Source<REST> UserAPI
  url: "http://api.example.com/users/{userId}"
  method: GET
  parameters:
    path:
      - userId: string = GetUser.userId;
  response:
    type: object
    entity: UserData
end
