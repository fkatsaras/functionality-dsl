Server Test
  host: "localhost"
  port: 8080
end

Entity DataWrapper
  attributes:
    - items: array;
end

Source<REST> RestSource
  url: "http://api.example.com/data"
  method: GET
  response:
    type: object
    entity: DataWrapper
end

Source<WS> WSSource
  channel: "ws://api.example.com/data"
  subscribe:
    type: object
    entity: DataWrapper  // ERROR: Same entity used for both REST and WS!
end

Entity Computed(DataWrapper)
  attributes:
    - count: integer = len(DataWrapper.items);
end

Endpoint<WS> LiveData
  channel: "/api/data"
  subscribe:
    type: object
    entity: Computed
end
