Server TestAPI
  host: "localhost"
  port: 8080
end

Entity User
  attributes:
    - id: string;
    - email: string;
    - name: string;
  source: UserDB
  expose:
    rest: "/api/users"
    operations: [list, read, create, update, delete]
    id_field: "id"
    readonly_fields: ["id"]
end

Source<REST> UserDB
  base_url: "http://user-service:9000/users"
  crud: standard
    entity: User
end
