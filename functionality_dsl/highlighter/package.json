{
  "name": "functionality-dsl-highlighter",
  "displayName": "Functionality DSL Highlighter",
  "publisher": "fotiskatsaras",
  "version": "0.0.1",
  "engines": { "vscode": "^1.70.0" },
  "license": "MIT",
  "repository": {"type": "git","url": "https://github.com/fkatsaras/functionality-dsl"},
  "contributes": {
    "languages": [
      {
        "id": "fdsl",
        "aliases": ["FDSL", "fdsl"],
        "extensions": [".fdsl", ".dsl", ".model"],
        "filenames": [
          "component.tx",
          "entity.tx",
          "endpoints.tx",
          "expression.tx",
          "model.tx",
          "server.tx",
          "utils.tx"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "fdsl",
        "scopeName": "source.fdsl",
        "path": "./syntaxes/fdsl.tmLanguage.json"
      }
    ]
  }
}
