import utils
import types
import datasource
import entity
import dto
import hook
import derived
import endpoint

Model:
    (imports+=Import)*
    (
        datasources      += Datasource
      | entities         += BackendEntity
      | frontendEntities += FrontendEntity
      | hooks            += Hook
      | derivedEntities  += DerivedEntity
      | endpoints        += Endpoint
    )*
;

Import: 'import' imported+=STRING ;