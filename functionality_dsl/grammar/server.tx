import utils

Server:
    'Server' name=ID
    (
        ('host:' host=STRING)
        ('port:' port=INT)
        ('base:' base=STRING)?
        ('cors:' cors+=STRING (',' cors+=STRING)*)?
        ('env:' env=ID)?          // (expects: dev | prod; or omit)
        ('loglevel:' loglevel=ID)?          // (expects: debug | info | error; or omit)
    )#
    'end'
;
