import utils

HttpVerb: 'GET' | 'POST' | 'PUT' | 'DELETE' | 'PATCH' ;

// Abstract union hook for Entity.source
Source:
      RESTEndpoint
    | WSEndpoint
;

RESTEndpoint:
    'RESTEndpoint' name=ID
    (
        'url:' url=STRING
        ('verb:' verb=HttpVerb)?
        ('headers:' headers=STRING)?
    )#
    'end'
;

WSEndpoint:
    'WSEndpoint' name=ID
    (
        'url:' url=STRING
        ('protocol:' protocol=STRING)?
    )#
    'end'
;
