import server
import entity
import methods
import security

// Binds HTTP base to an Entity with a set of methods.
Endpoint:
    'Endpoint' name=ID
    (
        ('base:' base=STRING)
        ('entity:' entity=[Entity])
        ('server:' server=[Server])?
        ('methods:' '-' methods+=[Method]['-'])?
        ('security' ':' security=[Security])?
    )#
    'end'
;
