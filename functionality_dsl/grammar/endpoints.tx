import utils
import integration
import entity

// ---------- INTERNAL (presentation) ----------
InternalEndpoint:
      InternalRESTEndpoint
    | InternalWSEndpoint
;

InternalRESTEndpoint:
  'InternalREST' name=ID
  (
    'entity:' entity=[Entity]
    ('path:' path=STRING)?
    ('summary:' summary=STRING)?
  )#
  'end'
;

InternalWSEndpoint:
  'InternalWS' name=ID
  (
    'entity:' entity=[Entity]
    ('path:' path=STRING)?
    ('summary:' summary=STRING)?
  )#
  'end'
;
