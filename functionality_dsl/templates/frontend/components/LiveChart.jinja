{% set ws_endpoint = props.get('streamPath') %}
<LiveChart
  name="{{ common.name }}"
  wsUrl="{{ ws_endpoint }}"
  {%- if props.get('seriesLabels') %}
  seriesLabels={{ "{" }}{{ props.get('seriesLabels') | tojson }}{{ "}" }}
  {%- endif %}
  {%- if props.get('xLabel') %}
  xLabel="{{ props.get('xLabel').get('text') if props.get('xLabel') else '' }}"
  xMeta={{ "{" }}{{ props.get('xLabel') | tojson }}{{ "}" }}
  {%- endif %}
  {%- if props.get('yLabel') %}
  yLabel="{{ props.get('yLabel').get('text') if props.get('yLabel') else '' }}"
  yMeta={{ "{" }}{{ props.get('yLabel') | tojson }}{{ "}" }}
  {%- endif %}
  {%- if props.get('windowSize') %}
  windowSize={{ props.get('windowSize') }}
  {%- endif %}
  {%- if props.get('height') %}
  height={{ props.get('height') }}
  {%- endif %}
>
</LiveChart>
